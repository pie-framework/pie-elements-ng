{
  "name": "@pie-element/local-esm-cdn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./embedded": {
      "types": "./dist/embedded.d.ts",
      "default": "./dist/embedded.js"
    },
    "./adapters/vite": {
      "types": "./dist/adapters/vite.d.ts",
      "default": "./dist/adapters/vite.js"
    },
    "./adapters/sveltekit": {
      "types": "./dist/adapters/sveltekit.d.ts",
      "default": "./dist/adapters/sveltekit.js"
    },
    "./adapters/connect": {
      "types": "./dist/adapters/connect.d.ts",
      "default": "./dist/adapters/connect.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:standalone": "bun test test/standalone.test.ts",
    "test:embedded": "bun test test/embedded.test.ts",
    "test:adapters": "bun test test/adapters.test.ts",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "es-module-lexer": "^2.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
    "@sveltejs/kit": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "@sveltejs/kit": {
      "optional": true
    }
  },
  "license": "MIT"
}
