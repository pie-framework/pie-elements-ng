{
  "name": "@pie-elements-ng/shared-mathml-to-latex",
  "version": "1.0.0",
  "description": "MathML to LaTeX converter (ESM) - internalized from @pie-framework/mathml-to-latex",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.9.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mathml",
    "latex",
    "convert",
    "parser",
    "mathml-to-latex",
    "esm"
  ],
  "license": "MIT"
}
